{% extends 'base.html.twig' %}

{% block title %}Teams{% endblock %}

{% block body %}
    <h1>Teams</h1>

    <ul>
        {% for team in teams %}
            <li>
                <a href="{{ path('app_team_show', {'id': team.id}) }}">{{ team.name }}</a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}